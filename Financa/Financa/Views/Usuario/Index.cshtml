@model IList<Financa.Entidades.Usuario>

@Html.ActionLink("Novo Usuario", "Form")


<table class="table table-hover" >
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>E-mail</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
        <tr>
            <td>@usuario.ID</td>
            <td>@usuario.Nome</td>
            <td>@usuario.Email</td>
        </tr>
        }
    </tbody>
</table>